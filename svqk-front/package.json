{
  "name": "svqk-front",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "gen-api-client": "dotenv cross-env-shell 'swagger-typescript-api -p $PUBLIC_BACKEND_URL/q/openapi -o src/lib/arch/api --disable-throw-on-error --custom-config tools/config.cjs --module-name-index 1' && pnpm copy-api",
    "copy-api": "cpx src/lib/arch/api/Api.ts ../svqk-e2etest/tests/api",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@felte/reporter-svelte": "^1.2.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/marked": "^6.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "less": "^4.2.1",
    "marked": "^15.0.4",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.16.0",
    "svelte-check": "^4.1.1",
    "sveltekit-i18n": "^2.4.2",
    "swagger-typescript-api": "^13.0.23",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  },
  "type": "module",
  "dependencies": {
    "@felte/validator-yup": "^1.1.4",
    "@picocss/pico": "^2.0.6",
    "dayjs": "^1.11.13",
    "felte": "^1.3.0",
    "inversify": "^6.2.1",
    "inversify-binding-decorators": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "yup": "^1.6.1",
    "yup-locale-ja": "^1.0.0"
  }
}