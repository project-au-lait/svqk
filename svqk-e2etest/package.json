{
  "name": "svqk-e2etest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "playwright-install": "playwright install",
    "e2etest": "cross-env NODE_OPTIONS=--no-experimental-strip-types playwright test --trace on",
    "playwright-show-report": "playwright show-report",
    "transform-report": "node tools/report-transformer.js",
    "dry-run": "cross-env DRY_RUN=true NODE_OPTIONS=--no-experimental-strip-types playwright test --reporter=list",
    "dry-run-csv": "cross-env DRY_RUN=true DRY_RUN_LOG=csv NODE_OPTIONS=--no-experimental-strip-types playwright test --reporter=list",
    "generate:i18n": "paraglide-js compile --project ./project.inlang --outdir ./tests/paraglide",
    "watch:paraglide": "chokidar tests/translations/*.json -c \"pnpm run generate:i18n\""
  },
  "keywords": [],
