  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.2.0",
    "@playwright/test": "^1.51.0",
    "@types/node": "^22.13.10",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "os-locale": "^6.0.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
