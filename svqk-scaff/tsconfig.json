{
  "compilerOptions": {
    "paths": {
      "*": ["./src/*"]
    },
    "module": "CommonJS",
    "target": "ES2022",
    "outDir": "./generators/app",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
